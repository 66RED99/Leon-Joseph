#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules/plasmo/bin/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules/plasmo/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules/plasmo/bin/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules/plasmo/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.2_@swc+helpers@0.5.6__@swc+helpers@0.5.6_lodash@4.17.21_postcss@8_rwr2p6nhz7wlh7ruhegnhbcjbu/node_modules:/mnt/e/Leon/CODING/chatgptwriter/chrome-extension-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/bin/index.mjs" "$@"
else
  exec node  "$basedir/../plasmo/bin/index.mjs" "$@"
fi
